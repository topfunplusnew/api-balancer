# 服务器配置
PORT=25052

# Redis配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# API配置 - 使用 API_{NAME}_BASE_URL、API_{NAME}_VERSION 和 API_{NAME}_TOKEN 格式
# 示例：Creatomate API
API_CREATOMATE_BASE_URL=https://api.creatomate.com
API_CREATOMATE_VERSION=v2
API_CREATOMATE_TOKEN=your_creatomate_api_token
# 或者使用 API_KEY 格式（二选一）
# API_CREATOMATE_API_KEY=your_creatomate_api_key

# 示例：Coze API
API_COZE_BASE_URL=https://api.coze.com
API_COZE_VERSION=v1
API_COZE_TOKEN=your_coze_api_token

# 示例：n8n API（示例地址请替换为你的实例）
API_N8N_BASE_URL=https://n8n.example.com
API_N8N_VERSION=api/v1
API_N8N_TOKEN=your_n8n_api_token

# 后续添加更多API时，按照以下格式：
# API_{NAME}_BASE_URL=https://api.example.com
# API_{NAME}_VERSION=v1
# API_{NAME}_TOKEN=your_api_token
# 注意：TOKEN 或 API_KEY 用于请求第三方API时的鉴权，用户的Token只用于访问本系统代理接口

# Supabase配置
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key
